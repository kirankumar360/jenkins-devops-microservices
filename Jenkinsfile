//scripted stage pipelines are optional.

//Declrative
pipeline {
	agent any
	stages {
		stage('Build') {
			steps {
				echo "Build"
			}
		}
		stage('Test') {
			steps {
	            echo "Test"
			}
		}
		stage('Integration Test') {
			steps {
	            echo "Integration Test"
			}
		}
	
	}
	
}
